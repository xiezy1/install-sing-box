{
    "log": {
        "level": "warn"
    },
    "dns": {
        "servers": [
            {
                "tag": "remote",
                "address": "1.1.1.1",
                "detour": "proxy"
            },
            {
                "tag": "local",
                "address": "223.5.5.5"
            }
        ],
        "rules": [
            {
                "outbound": "any",
                "server": "local",
                "disable_cache": true
            }
        ],
        "final": "remote"
    },
    "inbounds": [
        {
            "type": "mixed",
            "listen": "::",
            "listen_port": 10808
        }
    ],
    "outbounds": [
        {
            "type": "selector",
            "tag": "proxy",
            "outbounds": [
                "auto",
                "direct"
            ]
        },
        {
            "type": "urltest",
            "tag": "auto",
            "outbounds": [],
            "interval": "3m",
            "tolerance": 50
        },
        {
            "type": "direct",
            "tag": "direct"
        }
    ],
    "route": {
        "rules": [
            {
                "protocol": "dns",
                "action": "hijack-dns"
            },
            {
                "ip_is_private": true,
                "action": "direct"
            }
        ],
        "final": "proxy"
    }
}
