{
    "log": {
        "level": "warn"
    },
    "dns": {
        "servers": [
            {
                "tag": "remote",
                "address": "https://1.1.1.1/dns-query"
            },
            {
                "tag": "dns-local",
                "address": "223.5.5.5"
            }
        ],
        "rules": [
            {
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "server": "local",
                "disable_cache": true
            }
        ],
        "final": "remote"
    },
    "inbounds": [
        {
            "type": "mixed",
            "listen": "::",
            "listen_port": 10808
        }
    ],
    "outbounds": [
        {
            "type": "selector",
            "tag": "proxy",
            "outbounds": [
                "auto",
                "direct"
            ]
        },
        {
            "type": "urltest",
            "tag": "auto",
            "outbounds": [],
            "interval": "3m",
            "tolerance": 50
        },
        {
            "type": "direct",
            "tag": "direct"
        }
    ],
    "route": {
        "default_domain_resolver": "dns-local",
        "auto_detect_interface": true,
        "rules": [
            {
                "protocol": "dns",
                "action": "hijack-dns"
            },
            {
                "ip_is_private": true,
                "action": "direct"
            }
        ],
        "final": "proxy"
    },
    "experimental": {
        "clash_api": {
            "external_controller": "0.0.0.0:9090",
            "secret": "123456",
            "external_ui": "ui"
        }
    }
}